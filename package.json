{
  "name": "react-router-guarded-routes",
  "version": "0.4.0",
  "description": "a guard middleware for react-router v6",
  "keywords": [
    "react",
    "router",
    "guard"
  ],
  "license": "MIT",
  "author": {
    "name": "Col0ring",
    "email": "1561999073@qq.com"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rimraf dist && tsup",
    "dev": "tsup --watch",
    "prepare": "husky install",
    "lint": "npm-run-all --parallel lint:* && npm run format",
    "lint:eslint": "eslint . --fix --ext .ts,.tsx,.js,.jsx",
    "lint:stylelint": "stylelint **/*.css --fix --allow-empty-input",
    "format": "cross-env NODE_ENV=production prettier . --write --no-error-on-unmatched-pattern",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "types-kit": "^0.0.8"
  },
  "devDependencies": {
    "@col0ring/eslint-config": "^0.0.8",
    "@col0ring/prettier-config": "^0.0.2",
    "@col0ring/stylelint-config": "^0.0.8",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-angular": "^17.0.3",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.0.0",
    "react-router": "^6.3.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "stylelint": "^14.9.1",
    "ts-jest": "^28.0.7",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-router": "^6.0.0"
  }
